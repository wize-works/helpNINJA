{
    "inputs": [
        {
            "type": "promptString",
            "id": "supabase-access-token",
            "description": "Enter your Supabase Access Token",
            "password": true
        }
    ],
    "servers": {
        "supabase": {
            "command": "npx",
            "args": [
                "-y",
                "@supabase/mcp-server-supabase@latest",
                "--read-only",
                "--project-ref=adrokmnrceksbpwjusjj"
            ],
            "env": {
                "SUPABASE_ACCESS_TOKEN": "${input:supabase-access-token}"
            }
        }
    }
}